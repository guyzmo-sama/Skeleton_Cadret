{
    "name": "cadret/skeleton",
    "type": "project",
    "license": "MIT",
    "description": "Squelette pour le framework cadret",
    "minimum-stability": "dev",
    "autoload" : {
        "psr-4" : {
            "App\\" : "src/"     
        }
    },
    "repositories":[
        {
            "type": "package",
            "package": {
                "name": "guyzmo-sama/cadret",
                "version" : "1.3",
                "source": {
                    "url": "https://github.com/guyzmo-sama/cadret.git",
                    "type": "git",
                    "reference": "master"
                },
                "autoload": {
                    "psr-4": { "Cadret\\": ["src/"] }
                }
            }
        }
    ],
    "scripts": {
       
    },
    "require": {
        "guyzmo-sama/cadret": "^1.3",
        "twig/twig": "^3.0",
        "nikic/fast-route": "^1.3" 
    },
    "require-dev": {
        "symfony/var-dumper": "^5.1"
    }
}